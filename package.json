{
  "name": "MEAN-stack-authentication-minimongo",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Yann Guillerm @yanngv29 && Leonardo Maglio  @leota",
  "description": "MEAN stack authentication & minimongo boilerplate with Angular 6 and Angular Material.",
  "angular-cli": {},
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.0",
    "@angular/service-worker": "~6.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "angular-jwt": "^0.1.11",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.5.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "minimongo": "^6.0.0",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "rxjs": "^6.0.0",
    "tether": "1.4.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^8.0.2",
    "@angular/language-service": "^6.0.0",
    "@types/express": "^4.0.37",
    "@types/jasmine": "~2.5.54",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.92",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
